<!doctype html>
<html class="supports_line_clamp supports_flexbox no_touch" lang="en">
  <script language="JavaScript"> 
    // Force redirect to HTTPS
    var host = "slack.wopian.me";
    if ((host == window.location.host) && (window.location.protocol != "https:"))
      window.location.protocol = "https";
  </script>

  <head>
    <meta charset="utf-8">
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="superfish" content="nofish">
    <meta name="author" content="wopian">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Material Slack Themes" %></title>

    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.14/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.2.13/angular-ui-router.min.js"></script>

    <link href="/favicon-32.png" rel="shortcut icon" sizes="16x16 32x32 48x48" type="image/png">

    <link href="/stylesheets/rollup-client_core.css" rel="stylesheet" type="text/css">
    <link href="/stylesheets/rollup-client_primary.css" rel="stylesheet" type="text/css">
    <link href="/stylesheets/rollup-client_general.css" rel="stylesheet" type="text/css">
    <link href="/stylesheets/rollup-client_secondary.css" rel="stylesheet" type="text/css">
    <link href="/stylesheets/libs/lato-2-compressed.css" rel="stylesheet" type="text/css">
    <style type="text/css">
      :root #header + #content > #left > #rlblock_left { display: none !important; }
    </style>
    <style id="dynamic_content_max_width_rule" type="text/css">
      #msgs_div div.dynamic_content_max_width { max-width: 1287px; }
